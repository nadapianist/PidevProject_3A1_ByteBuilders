<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.Controllers.ShowHostelList">
    <children>
        <TableView fx:id="ListHostel" layoutX="51.0" layoutY="42.0" prefHeight="200.0" prefWidth="282.0">
            <columns>
                <TableColumn id="Name_HostelID" fx:id="Name_Hostel" prefWidth="92.0" text="Hostel Name" />
                <TableColumn id="NBstarsID" fx:id="NBstars" prefWidth="102.0" text="NBStars" />
                <TableColumn id="InfoID" fx:id="Info" prefWidth="92.0" text="Info" />
            </columns>
        </TableView>
        <Button fx:id="AddButton" layoutX="360.0" layoutY="48.0" mnemonicParsing="false" onAction="#NextPage" text=" Add" />
        <Button fx:id="DeleteButton" layoutX="354.0" layoutY="90.0" mnemonicParsing="false" onAction="#DeletePage" text=" delete" />
        <Button fx:id="UpdateButton" layoutX="351.0" layoutY="142.0" mnemonicParsing="false" onAction="#UpdatePage" text=" Update" />
    </children>
</AnchorPane>
