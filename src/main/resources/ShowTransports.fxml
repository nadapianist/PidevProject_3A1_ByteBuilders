<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.Controller.ShowTransports">
   <children>
      <TableView fx:id="TransportList" layoutX="128.0" layoutY="84.0" prefHeight="200.0" prefWidth="394.0">
        <columns>
          <TableColumn fx:id="brand" prefWidth="75.0" text="brand" />
          <TableColumn fx:id="type" prefWidth="75.0" text=" type" />
            <TableColumn fx:id="distance" prefWidth="75.0" text=" distance" />
            <TableColumn fx:id="ChargingTime" prefWidth="87.0" text="Charging Time" />
            <TableColumn fx:id="IDtourist" prefWidth="87.0" text=" ID tourist" />
        </columns>
      </TableView>
      <Button fx:id="AddPageButton" layoutX="128.0" layoutY="42.0" mnemonicParsing="false" onAction="#ToAddPage" text=" Add" />
      <Button fx:id="UpdatePageButton" layoutX="202.0" layoutY="42.0" mnemonicParsing="false" onAction="#ToUpdatePage" text=" Update" />
      <Button fx:id="DeletePageButton" layoutX="286.0" layoutY="42.0" mnemonicParsing="false" onAction="#ToDeletePage" text=" Delete" />
   </children>
</AnchorPane>
