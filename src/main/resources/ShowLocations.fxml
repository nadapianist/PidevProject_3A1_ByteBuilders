<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="616.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.Controller.ShowLocations">
   <children>
      <TableView fx:id="LocationList" layoutX="33.0" layoutY="60.0" prefHeight="200.0" prefWidth="450.0">
        <columns>
          <TableColumn fx:id="NameLoc" prefWidth="75.0" text=" Name" />
          <TableColumn fx:id="InfoLoc" prefWidth="75.0" text=" info" />
            <TableColumn fx:id="categoryLoc" prefWidth="73.0" text="category" />
            <TableColumn fx:id="ActivityLocId" prefWidth="77.0" text="activity" />
            <TableColumn fx:id="LocHostelId" prefWidth="75.0" text="hostel" />
            <TableColumn fx:id="ratingLoc" prefWidth="75.0" text="rating" />
        </columns>
      </TableView>
      <Button fx:id="AddButton" layoutX="496.0" layoutY="60.0" mnemonicParsing="false" onAction="#ToAddPage" text=" Add Location" />
      <Button fx:id="EditButton" layoutX="496.0" layoutY="99.0" mnemonicParsing="false" onAction="#ToEditPage" text=" Update Location" />
      <Button fx:id="DeleteLocButton" layoutX="499.0" layoutY="148.0" mnemonicParsing="false" onAction="#ToDeletePage" text=" Delete Location" />
   </children>
</AnchorPane>
